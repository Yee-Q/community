    <update id="update" parameterType="com.yeexang.community.pojo.po.User">
        update y_c_dict d
        <set>
            <include refid="set_condition"/>
        </set>
        <where>
            <if test="id != null and id != ''">
                AND d.id = #{id}
            </if>
            <if test="dictId != null and dictId != ''">
                AND d.dictId = #{dictId}
            </if>
        </where>
    </update>

    <select id="select" parameterType="com.yeexang.community.pojo.po.Dict" resultType="com.yeexang.community.pojo.po.Dict">
        select <include refid="y_c_dict"/> from y_c_dict d
