    </sql>

    <sql id="where_condition">
        <if test="id != null and id != ''">
            AND d.id = #{id}
        </if>
        <if test="dictId != null and dictId != ''">
            AND d.dict_id = #{dictId}
        </if>
        <if test="label != null and label != ''">
